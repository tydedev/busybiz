{
  "$schema": "https://schemas.wp.org/wp/6.9/theme.json",
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "color": "#6366f1",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#312e81",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#f97316",
          "name": "Terziary",
          "slug": "terziary"
        },
        {
          "color": "#fafafa",
          "name": "Light",
          "slug": "light"
        },
        {
          "color": "#e2e8f0",
          "name": "Gray",
          "slug": "gray"
        },
        {
          "color": "#0f172a",
          "name": "Dark",
          "slug": "dark"
        }
      ]
    },
    "layout": {
      "contentSize": "620px",
      "wideSize": "1200px"
    },
    "spacing": {
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "fluid": true,
          "name": "Extra Small",
          "slug": "x-small",
          "size": "0.75rem"
        },
        {
          "fluid": true,
          "name": "Small",
          "slug": "small",
          "size": "0.875rem"
        },
        {
          "fluid": true,
          "name": "Medium",
          "slug": "medium",
          "size": "1.2rem"
        },
        {
          "fluid": true,
          "name": "Large",
          "slug": "large",
          "size": "2.125rem"
        },
        {
          "fluid": true,
          "name": "Extra Large",
          "slug": "x-large",
          "size": "3.25rem"
        },
        {
          "fluid": true,
          "name": "XX Large",
          "slug": "xx-large",
          "size": "4.25rem"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "'Inter', sans-serif",
          "name": "Inter",
          "slug": "inter",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontWeight": "400 700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/inter/Inter-VariableFont_opsz,wght.ttf"
              ]
            },
            {
              "fontFamily": "Inter",
              "fontWeight": "400 700",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/inter/Inter-Italic-VariableFont_opsz,wght.ttf"
              ]
            }
          ]
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "tydedev/project-website": {
        "variations": {
          "button": {
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--primary)"
            }
          },
          "link": {
            "typography": {
              "fontSize": "var(--wp--preset--font-size--x-small)"
            },
            "css": ":hover{background:var(--wp--preset--color--secondary);text-decoration:none;}",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--dark)"
            },
            "border": {
              "radius": "4px",
              "color": "var(--wp--preset--color--gray)"
            },
            "spacing": {
              "padding": {
                "top": "0.3rem",
                "bottom": "0.3rem",
                "left": "0.5rem",
                "right": "0.5rem"
              }
            }
          }
        }
      },
      "core/heading": {
        "variations": {
          "checkmark": {}
        }
      },
      "core/post-excerpt": {
        "elements": {
          "button": {
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--primary)"
            }
          },
          "link": {
            "typography": {
              "fontSize": "var(--wp--preset--font-size--x-small)"
            },
            "css": ":hover{background:var(--wp--preset--color--secondary);text-decoration:none;}",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--dark)"
            },
            "border": {
              "radius": "4px",
              "color": "var(--wp--preset--color--gray)"
            },
            "spacing": {
              "padding": {
                "top": "0.3rem",
                "bottom": "0.3rem",
                "left": "0.5rem",
                "right": "0.5rem"
              }
            }
          }
        }
      },
      "core/search": {
        "elements": {
          "textInput": {
            "border": {
              "radius": "8px",
              "color": "var(--wp--preset--color--gray)"
            }
          },
          "button": {
            "border": {
              "radius": "8px"
            },
            "css": ":hover{background:var(--wp--preset--color--secondary)}",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--primary)"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem"
              }
            }
          }
        }
      },
      "core/button": {
        "border": {
          "radius": "8px"
        },
        "css": ":hover{background:var(--wp--preset--color--secondary)}",
        "color": {
          "text": "var(--wp--preset--color--light)",
          "background": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem",
            "bottom": "0.5rem"
          }
        },
        "variations": {
          "contact": {
            "css": ":hover{background:var(--wp--preset--color--secondary)};",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--primary)"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem"
              }
            }
          },
          "contact-outline": {
            "color": {
              "text": "var(--wp--preset--color--secondary)",
              "background": "transparent"
            },
            "css": ":hover{border-color:var(--wp--preset--color--dark); color:var(--wp--preset--color--dark)}",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "var(--wp--preset--color--secondary)"
            },
            "spacing": {
              "padding": {
                "top": "0.4rem",
                "bottom": "0.4rem"
              }
            }
          },
          "call": {
            "css": ":hover{background:var(--wp--preset--color--secondary)};",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--primary)"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem"
              }
            }
          },
          "call-outline": {
            "color": {
              "text": "var(--wp--preset--color--secondary)",
              "background": "transparent"
            },
            "css": ":hover{border-color:var(--wp--preset--color--dark); color:var(--wp--preset--color--dark)}",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "var(--wp--preset--color--secondary)"
            },
            "spacing": {
              "padding": {
                "top": "0.4rem",
                "bottom": "0.4rem"
              }
            }
          },
          "secondary": {
            "css": ":hover{background:var(--wp--preset--color--dark)}",
            "color": {
              "text": "var(--wp--preset--color--light)",
              "background": "var(--wp--preset--color--secondary)"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem"
              }
            }
          },
          "outline": {
            "color": {
              "text": "var(--wp--preset--color--secondary)",
              "background": "transparent"
            },
            "css": ":hover{border-color:var(--wp--preset--color--dark); color:var(--wp--preset--color--dark)}",
            "border": {
              "width": "2px",
              "style": "solid",
              "color": "var(--wp--preset--color--secondary)"
            },
            "spacing": {
              "padding": {
                "top": "0.4rem",
                "bottom": "0.4rem"
              }
            }
          }
        }
      },
      "core/paragraph": {
        "variations": {
          "address": {},
          "email:": {},
          "phone": {},
          "checkmark": {}
        },
        "spacing": {
          "margin": {
            "top": "0.5rem",
            "bottom": "0.5rem"
          }
        }
      },
      "core/list": {
        "spacing": {
          "margin": {
            "top": "0.5rem",
            "bottom": "0.5rem"
          }
        }
      },
      "core/site-title": {
        "css": "& :hover{text-decoration:none;}"
      },
      "core/post-title": {
        "color": {
          "text": "var(--wp--preset--color--dark)"
        },
        "css": "& :hover{text-decoration:none; color:var(--wp--preset--color--dark);}"
      }
    },
    "elements": {
      "link": {
        "typography": {
          "fontWeight": "600",
          "textDecoration": "none"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        },
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          },
          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
        }
      },
      "heading": {
        "typography": {
          "fontWeight": "600",
          "lineHeight": "1.5"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)",
          "fontWeight": "500",
          "textTransform": "uppercase"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "textTransform": "uppercase"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      }
    },
    "spacing": {
      "blockGap": "0"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "lineHeight": "1.5"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    },
    {
      "area": "uncategorized",
      "name": "sidebar",
      "title": "Sidebar"
    }
  ],
  "customTemplates": [
    {
      "name": "blog-with-sidebar",
      "title": "Blog with Sidebar",
      "postTypes": ["page"]
    }
  ],
  "version": 3
}
